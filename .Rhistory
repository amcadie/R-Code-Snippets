df <- data.frame(loc = c("Hollywood, FL", "New York City", "Seattle"))
df$loc <- as.character(df$loc)
for(i in 1:length(df$loc)){
df$lon[i] <- geocode(df$loc[i])$lon
df$lat[i] <- geocode(df$loc[i])$lat
}
worldmap <- ggplot(world, aes(x=long, y = lat, group = group))
worldmap <- worldmap + geom_polygon(fill="#1A1A1A", colour="#333333")
worldmap <- worldmap + geom_point(data = df, aes(x = lon, y = lat, group = NULL),
size = 5, color = "green")
worldmap <- worldmap + geom_segment(data = df, aes(x = -122.33207, y = 47.60621,
xend = lon, yend = lat,
group = NULL), color = "red")
worldmap <- worldmap + theme(panel.grid.major = element_line(color = "#2D333C"),
panel.grid.minor = element_line(color = "#2D333C"),
panel.background = element_rect(fill = "#2D333C"))
worldmap <- worldmap + coord_map(project = "miller")
worldmap
