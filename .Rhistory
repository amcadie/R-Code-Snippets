world <- map_data("world")
df <- data.frame(loc = c("Hollywood, FL", "New York City", "Seattle", "Moscow", "Tokyo"))
df$loc <- as.character(df$loc)
for(i in 1:length(df$loc)){
df$lon[i] <- geocode(df$loc[i])$lon
df$lat[i] <- geocode(df$loc[i])$lat
}
